{
	"name": "dnd-spell-organizer",
	"license": "MIT",
	"version": "0.4.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"build-test": "tsc && vite build --mode staging",
		"preview": "vite preview",
		"prettier": "prettier --config ./.prettierrc --ignore-path  ./.prettierignore --write \"**/*.ts*\" \"**/*.scss\" \"*.json\" \"*.md\"",
		"prettier-check": "prettier --list-different --config ./.prettierrc --ignore-path  ./.prettierignore \"**/*.ts*\" \"**/*.scss\" \"*.json\" \"*.md\"",
		"lint-check": "yarn prettier-check && yarn eslint && yarn stylelint-check",
		"lint": "yarn prettier; yarn eslint; yarn stylelint",
		"eslint": "eslint '*/**/*.{js,ts,tsx}'",
		"stylelint": "stylelint --config ./.stylelintrc --fix \"src/**/*.scss\" --color --cache",
		"stylelint-check": "stylelint --config ./.stylelintrc \"src/**/*.scss\" --color --cache",
		"outdated": "yarn upgrade-interactive"
	},
	"dependencies": {
		"@ag-grid-community/client-side-row-model": "^29.3.5",
		"@ag-grid-community/core": "^29.3.5",
		"@ag-grid-community/react": "^29.3.5",
		"@popperjs/core": "^2.11.8",
		"@rollbar/react": "^0.11.1",
		"bootstrap": "^5.3.0-alpha3",
		"path": "^0.12.7",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-bootstrap": "^2.7.4",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.11.2",
		"rollbar": "^2.26.1"
	},
	"devDependencies": {
		"@swc/core": "1.3.60",
		"@swc/helpers": "^0.5.1",
		"@types/mocha": "^10.0.1",
		"@types/node": "^20.2.5",
		"@types/react": "^18.2.7",
		"@types/react-dom": "^18.2.4",
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"@vitejs/plugin-react-swc": "^3.3.1",
		"cypress": "^12.13.0",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"postcss": "^8.4.24",
		"postcss-scss": "^4.0.6",
		"prettier": "^2.8.8",
		"sass": "^1.62.1",
		"stylelint": "^15.6.2",
		"typescript": "^5.0.4",
		"vite": "4.3.9"
	},
	"packageManager": "yarn@3.5.1"
}
