{
	"name": "dnd-spell-organizer",
	"license": "MIT",
	"version": "0.5.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"build-test": "tsc && vite build --mode staging",
		"preview": "vite preview",
		"prettier": "prettier --config ./.prettierrc --ignore-path  ./.prettierignore --write \"**/*.ts*\" \"**/*.scss\" \"*.json\" \"*.md\"",
		"prettier-check": "prettier --list-different --config ./.prettierrc --ignore-path  ./.prettierignore \"**/*.ts*\" \"**/*.scss\" \"*.json\" \"*.md\"",
		"lint-check": "yarn prettier-check && yarn eslint && yarn stylelint-check",
		"lint": "yarn prettier; yarn eslint; yarn stylelint",
		"eslint": "eslint '*/**/*.{js,ts,tsx}'",
		"stylelint": "stylelint --config ./.stylelintrc --fix \"src/**/*.scss\" --color --cache",
		"stylelint-check": "stylelint --config ./.stylelintrc \"src/**/*.scss\" --color --cache",
		"outdated": "yarn upgrade-interactive"
	},
	"dependencies": {
		"@ag-grid-community/client-side-row-model": "^31.0.1",
		"@ag-grid-community/core": "^31.0.1",
		"@ag-grid-community/react": "^31.0.1",
		"@popperjs/core": "^2.11.8",
		"@rollbar/react": "^0.11.2",
		"ag-grid-community": "^31.0.3",
		"bootstrap": "^5.3.2",
		"path": "^0.12.7",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-bootstrap": "^2.9.2",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.21.1",
		"rollbar": "^2.26.2"
	},
	"devDependencies": {
		"@swc/core": "1.3.105",
		"@swc/helpers": "^0.5.3",
		"@types/mocha": "^10.0.6",
		"@types/node": "^20.10.4",
		"@types/react": "18.2.48",
		"@types/react-dom": "^18.2.18",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.21.0",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"cypress": "^13.6.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.2",
		"eslint-plugin-react": "^7.33.2",
		"postcss": "^8.4.33",
		"postcss-scss": "^4.0.9",
		"prettier": "^3.1.1",
		"sass": "^1.69.7",
		"stylelint": "^16.1.0",
		"typescript": "^5.3.3",
		"vite": "5.0.12"
	},
	"packageManager": "yarn@3.5.1"
}
