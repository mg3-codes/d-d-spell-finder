{
	"name": "dnd-spell-organizer",
	"license": "MIT",
	"version": "0.5.2",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"build-test": "tsc && vite build --mode staging",
		"preview": "vite preview",
		"prettier": "prettier --config ./.prettierrc --ignore-path  ./.prettierignore --write \"**/*.ts*\" \"**/*.scss\" \"*.json\" \"*.md\"",
		"prettier-check": "prettier --list-different --config ./.prettierrc --ignore-path  ./.prettierignore \"**/*.ts*\" \"**/*.scss\" \"*.json\" \"*.md\"",
		"lint-check": "yarn prettier-check && yarn eslint && yarn stylelint-check",
		"lint": "yarn prettier; yarn eslint; yarn stylelint",
		"eslint": "eslint '*/**/*.{js,ts,tsx}'",
		"stylelint": "stylelint --config ./.stylelintrc --fix \"src/**/*.scss\" --color --cache",
		"stylelint-check": "stylelint --config ./.stylelintrc \"src/**/*.scss\" --color --cache",
		"outdated": "yarn upgrade-interactive"
	},
	"dependencies": {
		"@ag-grid-community/client-side-row-model": "^31.0.3",
		"@ag-grid-community/core": "^31.0.3",
		"@ag-grid-community/react": "^31.0.3",
		"@popperjs/core": "^2.11.8",
		"@rollbar/react": "^0.11.2",
		"ag-grid-community": "^31.1.1",
		"bootstrap": "^5.3.2",
		"path": "^0.12.7",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-bootstrap": "^2.10.1",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.22.1",
		"rollbar": "^2.26.4"
	},
	"devDependencies": {
		"@swc/core": "1.4.16",
		"@swc/helpers": "^0.5.8",
		"@types/mocha": "^10.0.6",
		"@types/node": "^20.11.19",
		"@types/react": "18.2.73",
		"@types/react-dom": "^18.2.19",
		"@typescript-eslint/eslint-plugin": "^7.7.0",
		"@typescript-eslint/parser": "^7.7.1",
		"@vitejs/plugin-react-swc": "^3.6.0",
		"cypress": "^13.13.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.34.1",
		"postcss": "^8.4.35",
		"postcss-scss": "^4.0.9",
		"prettier": "^3.2.5",
		"sass": "^1.75.0",
		"stylelint": "^16.2.1",
		"typescript": "^5.4.3",
		"vite": "5.1.7"
	},
	"packageManager": "yarn@3.5.1"
}
