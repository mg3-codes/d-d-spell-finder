{
	"name": "dnd-spell-organizer",
	"license": "MIT",
	"version": "0.5.4",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"build-test": "tsc && vite build --mode staging",
		"preview": "vite preview",
		"prettier": "prettier --config ./.prettierrc --ignore-path  ./.prettierignore --write \"**/*.ts*\" \"**/*.scss\" \"*.json\" \"*.md\"",
		"prettier-check": "prettier --list-different --config ./.prettierrc --ignore-path  ./.prettierignore \"**/*.ts*\" \"**/*.scss\" \"*.json\" \"*.md\"",
		"lint-check": "yarn prettier-check && yarn eslint && yarn stylelint-check",
		"lint": "yarn prettier; yarn eslint; yarn stylelint",
		"eslint": "eslint",
		"stylelint": "stylelint --config ./.stylelintrc --fix \"src/**/*.scss\" --color --cache",
		"stylelint-check": "stylelint --config ./.stylelintrc \"src/**/*.scss\" --color --cache",
		"outdated": "yarn upgrade-interactive"
	},
	"dependencies": {
		"@ag-grid-community/client-side-row-model": "^32.1.0",
		"@ag-grid-community/core": "^32.1.0",
		"@ag-grid-community/react": "^32.1.0",
		"@popperjs/core": "^2.11.8",
		"@rollbar/react": "^0.12.0-beta",
		"ag-grid-community": "^32.1.0",
		"bootstrap": "^5.3.3",
		"path": "^0.12.7",
		"prop-types": "^15.8.1",
		"react": "^18.3.1",
		"react-bootstrap": "^2.10.4",
		"react-dom": "^18.3.1",
		"react-router-dom": "^6.27.0",
		"rollbar": "^2.26.4"
	},
	"devDependencies": {
		"@swc/core": "1.9.3",
		"@swc/helpers": "^0.5.15",
		"@types/mocha": "^10.0.10",
		"@types/node": "^22.9.2",
		"@types/react": "18.3.12",
		"@types/react-dom": "^18.3.1",
		"@typescript-eslint/eslint-plugin": "^8.15.0",
		"@typescript-eslint/parser": "^8.15.0",
		"@vitejs/plugin-react-swc": "^3.7.1",
		"cypress": "^13.15.1",
		"eslint": "^9.15.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-react": "^7.37.2",
		"postcss": "^8.4.49",
		"postcss-scss": "^4.0.9",
		"prettier": "^3.3.3",
		"sass": "^1.81.0",
		"stylelint": "^16.10.0",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.15.0",
		"vite": "5.4.11"
	},
	"packageManager": "yarn@3.5.1"
}
