{
	"name": "dnd-spell-organizer",
	"license": "MIT",
	"version": "0.4.13",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"build-test": "tsc && vite build --mode staging",
		"preview": "vite preview",
		"prettier": "prettier --config ./.prettierrc --ignore-path  ./.prettierignore --write \"**/*.ts*\" \"**/*.scss\" \"*.json\" \"*.md\"",
		"prettier-check": "prettier --list-different --config ./.prettierrc --ignore-path  ./.prettierignore \"**/*.ts*\" \"**/*.scss\" \"*.json\" \"*.md\"",
		"lint-check": "yarn prettier-check && yarn eslint && yarn stylelint-check",
		"lint": "yarn prettier; yarn eslint; yarn stylelint",
		"eslint": "eslint '*/**/*.{js,ts,tsx}'",
		"stylelint": "stylelint --config ./.stylelintrc --fix \"src/**/*.scss\" --color --cache",
		"stylelint-check": "stylelint --config ./.stylelintrc \"src/**/*.scss\" --color --cache",
		"outdated": "yarn upgrade-interactive"
	},
	"dependencies": {
		"@ag-grid-community/client-side-row-model": "^30.1.0",
		"@ag-grid-community/core": "^30.1.0",
		"@ag-grid-community/react": "^30.1.0",
		"@popperjs/core": "^2.11.8",
		"@rollbar/react": "^0.11.2",
		"bootstrap": "^5.3.2",
		"path": "^0.12.7",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-bootstrap": "^2.8.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.15.0",
		"rollbar": "^2.26.2"
	},
	"devDependencies": {
		"@swc/core": "1.3.76",
		"@swc/helpers": "^0.5.2",
		"@types/mocha": "^10.0.1",
		"@types/node": "^18.17.14",
		"@types/react": "^18.2.21",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^6.7.2",
		"@typescript-eslint/parser": "^6.5.0",
		"@vitejs/plugin-react-swc": "^3.3.2",
		"cypress": "^12.17.3",
		"eslint": "^8.49.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-react": "^7.33.2",
		"postcss": "^8.4.29",
		"postcss-scss": "^4.0.7",
		"prettier": "^3.0.2",
		"sass": "^1.68.0",
		"stylelint": "^15.10.3",
		"typescript": "^5.2.2",
		"vite": "4.4.9"
	},
	"packageManager": "yarn@3.5.1"
}
